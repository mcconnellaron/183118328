(fun (isodd n)
    (if (< n 0)
        (isodd (* -1 n))
        (if (= n 0)
            false
            (iseven (sub1 n))
        )
    )
)
(fun (iseven n)
    (if (< n 0)
        (iseven (* -1 n))
        (if (= n 0)
            true
            (isodd (sub1 n))
        )
    )
)

(iseven 1000000)